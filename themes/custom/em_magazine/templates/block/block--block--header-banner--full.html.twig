{%
set classes = [
'block',
'block-' ~ configuration.provider|clean_class,
'block-' ~ plugin_id|clean_class,
]
%}

{% set attributes = attributes.addClass(classes) %}

{% block block %}
  <div{{ attributes }}>
    {% block content %}
      <a id="bnr" href="{{ content.field_header_link|render|striptags|trim }}" style="width:100%; height:90px;z-index:11; position: relative;z-index: 99;">
      	<img class="classTitle" src="{{ content.field_header_banner_image|render|striptags|trim }}">
      </a>
    {% endblock %}
  </div>
{% endblock %}

